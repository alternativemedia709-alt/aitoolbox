{
  "name": "aitoolbox-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "services/*",
    "worker",
    "shared/*"
  ],
  "scripts": {
    "dev": "docker-compose up",
